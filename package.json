{
  "name": "template-vite-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "dev:uat": "vite --host --mode uat",
    "build:uat": "vite build --mode uat",
    "build:tsc": "tsc && vite build",
    "preview": "vite preview",
    "eslint": "eslint src --fix --ext .js,.jsx,.ts,.tsx",
    "stylelint": "stylelint **/*.{css,less,scss} --fix",
    "prettier": "prettier -c --write \"**/*\"",
    "prepare": "husky install",
    "commitlint": "commitlint --config .commitlintrc.cjs -e -V",
    "preinstall": "npx only-allow pnpm",
    "swagger": "node swagger.config.js"
  },
  "dependencies": {
    "@ant-design/pro-components": "^2.6.35",
    "@hbwow/components": "^2.13.0",
    "@hbwow/hooks": "^2.5.0",
    "@hbwow/pdf-viewer": "^1.0.0",
    "@hbwow/router-animation": "^0.0.2",
    "@hbwow/utils": "^2.2.0",
    "@hbwow/validate-antd": "^1.0.0",
    "@loadable/component": "^5.15.3",
    "@tanstack/react-query": "^4.36.1",
    "@tanstack/react-query-devtools": "^4.36.1",
    "antd": "^5.13.2",
    "axios": "^1.6.0",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.17.0"
  },
  "devDependencies": {
    "@ant-design/cssinjs": "^1.18.4",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@hbwow/lints": "^2.1.1",
    "@tanstack/eslint-plugin-query": "^5.0.5",
    "@types/node": "^18.18.7",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@vitejs/plugin-react-swc": "^3.4.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.52.0",
    "free-swagger": "^5.4.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "rollup-plugin-visualizer": "^5.9.2",
    "stylelint": "^15.11.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-static-copy": "^0.17.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "eslint --config .eslintrc.cjs",
    "*.{css,less,scss}": "stylelint --config .stylelintrc.cjs",
    "*.{js,jsx,ts,tsx,less,md,json}": "prettier --write"
  }
}
