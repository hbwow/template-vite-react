{
  "name": "template-vite-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "eslint": "eslint src --fix --ext .js,.jsx,.ts,.tsx",
    "stylelint": "stylelint **/*.{css,less,scss} --fix",
    "prettier": "prettier -c --write \"**/*\"",
    "prepare": "husky install",
    "commitlint": "commitlint --config .commitlintrc.cjs -e -V",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@hbwow/lints": "^2.1.0",
    "@types/node": "^18.17.15",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.49.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "stylelint": "^15.10.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "eslint --config .eslintrc.cjs",
    "*.{css,less,scss}": "stylelint --config .stylelintrc.cjs",
    "*.{js,jsx,ts,tsx,less,md,json}": "prettier --write"
  }
}
